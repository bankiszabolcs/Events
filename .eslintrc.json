{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "standard",
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "requireConfigFile": false,
    "babelOptions": {
      "plugins": ["@babel/plugin-syntax-import-assertions"]
    }
  },
  "rules": {
    "comma-dangle": "off",
    "space-before-function-paren": "off",
    "semi": "off",
    "quotes": "off"
  }
}
